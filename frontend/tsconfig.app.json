{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./src",
    "paths": {
      "@pages/*": ["app/pages/*"],
      "@components/*": ["app/components/*"],
      "@models/*": ["app/models/*"],
      "@services/*": ["app/services/*"]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts"
  ]
}

